#!/bin/sh

sysled_4_blink() {
    sysled 1; usleep 100000
    sysled 0; usleep 100000
    sysled 1; usleep 100000
    sysled 0; usleep 100000
    sysled 1; usleep 100000
    sysled 0; usleep 100000
    sysled 1; usleep 100000
    sysled 0
}

year=`date +'%Y'`
while [ $year -lt 2015 ]; do
    sysled_4_blink
    date +'%F %T'
    year=`date +'%Y'`
    sleep 1
done
